{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "touhou_launcher_reborn",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "all": true,
        "open": true,
        "scope": [
          {
            "name": "wine",
            "cmd": "/home/wearr/.local/share/lutris/runners/wine/GE-Proton7-48/bin/wine",
            "args": true
          }
        ]
      },
      "dialog": {
        "all": true,
        "ask": true,
        "confirm": true,
        "message": true,
        "open": true,
        "save": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "dev.wearr.TLR",
      "targets": "all",
      "copyright": "MIT"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Touhou Launcher Reborn",
        "width": 1000,
        "height": 800,
        "minWidth": 1000,
        "minHeight": 800,
        "center": true,
        "theme": "Dark"
      }
    ]
  }
}
