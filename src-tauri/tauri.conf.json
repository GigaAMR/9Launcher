{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "copyright": "MIT",
    "targets": "all"
  },
  "productName": "9launcher",
  "mainBinaryName": "9launcher",
  "version": "1.3.2",
  "identifier": "dev.wearr.9launcher",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "9Launcher",
        "width": 1000,
        "height": 800,
        "minWidth": 1000,
        "minHeight": 800,
        "center": true,
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": null
    }
  }
}